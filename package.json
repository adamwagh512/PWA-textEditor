  "bugs": {
    "url": "https://github.com/adamwagh512/PWA-textEditor/issues"
  },
  "homepage": "https://github.com/adamwagh512/PWA-textEditor#readme",
  "dependencies": {
    "concurrently": "^7.3.0",
    "express": "^4.18.1",
    "if-env": "^1.0.4",
    "nodemon": "^2.0.19"
  }
}
